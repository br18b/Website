Cell[BoxData[
 RowBox[{
  RowBox[{"(*", "Parameters", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"y0", "=", 
     RowBox[{"2", "-", "EulerGamma", "-", 
      RowBox[{"Log", "[", "2", "]"}]}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"\[CapitalSigma]", "=", 
     RowBox[{"Sqrt", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "/", "4"}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Pi", "^", "2"}], "-", "8"}], ")"}]}], "]"}]}], ";"}], "\n", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "Characteristic", " ", "function", " ", "of", " ", "the", " ", 
      "standardized", " ", "log"}], "-", 
     RowBox[{"Mach", " ", "distribution"}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"phiY", "[", "t_", "]"}], ":=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"4", " ", 
         RowBox[{"Exp", "[", 
          RowBox[{"EulerGamma", "-", "2"}], "]"}]}], ")"}], "^", 
       RowBox[{"(", 
        RowBox[{"I", " ", 
         RowBox[{"t", "/", "\[CapitalSigma]"}]}], ")"}]}], "*", 
      RowBox[{"(", 
       RowBox[{"2", "/", 
        RowBox[{"Sqrt", "[", "Pi", "]"}]}], ")"}], "*", 
      RowBox[{"Gamma", "[", 
       RowBox[{
        RowBox[{"3", "/", "2"}], "+", 
        RowBox[{"I", " ", 
         RowBox[{"t", "/", "\[CapitalSigma]"}]}]}], "]"}]}]}], ";"}], "\n", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"Characteristic", " ", "function", " ", "of", " ", "s"}], "=", 
     RowBox[{"sum", " ", 
      RowBox[{"y_i", "/", "sqrt"}], 
      RowBox[{"(", "n", ")"}]}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"phi", "[", 
      RowBox[{"n_", ",", "t_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"phiY", "[", 
        RowBox[{"t", "/", 
         RowBox[{"Sqrt", "[", "n", "]"}]}], "]"}], ")"}], "^", "n"}]}], ";"}],
    "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "PDF", " ", "via", " ", "inverse", " ", "Fourier", " ", "transform"}], 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"fPDF", "[", 
      RowBox[{"n_", ",", "s_"}], "]"}], ":=", 
     RowBox[{"Re", "[", 
      RowBox[{
       RowBox[{"NIntegrate", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Exp", "[", 
           RowBox[{
            RowBox[{"-", "I"}], " ", "t", " ", "s"}], "]"}], "*", 
          RowBox[{"phi", "[", 
           RowBox[{"n", ",", "t"}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", 
           RowBox[{"-", "100"}], ",", "100"}], "}"}], ",", 
         RowBox[{"Method", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{"\"\<GlobalAdaptive\>\"", ",", 
            RowBox[{"\"\<MaxErrorIncreases\>\"", "\[Rule]", "100"}]}], 
           "}"}]}], ",", 
         RowBox[{"MaxRecursion", "\[Rule]", "15"}], ",", 
         RowBox[{"AccuracyGoal", "\[Rule]", "6"}], ",", 
         RowBox[{"PrecisionGoal", "\[Rule]", "6"}]}], "]"}], "/", 
